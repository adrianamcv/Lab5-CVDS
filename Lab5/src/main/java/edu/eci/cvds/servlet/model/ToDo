package edu.eci.cvds.servlet;

import java.util.*;

public class ToDo {
    private int userId;
    private int id;
    private String title;
    private boolean completed;

    public void setUserId(newUserId int){
        userId=newUserId;
    }

    public int getUserId(){
        return userId;
    }

    public void setId(newId int){
        id=newId;
    }

    public int getId(){
        return id;
    }

    public void setTitle(newTitle String){
        title=newTitle;
    }

    public String getTitle(){
        return title;
    }

    public void setCompleted(newCompleted boolean){
        completed=newCompleted;
    }

    public boolean getCompleted(){
        return completed;
    }

}